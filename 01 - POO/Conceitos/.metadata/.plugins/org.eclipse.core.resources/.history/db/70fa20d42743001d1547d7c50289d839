package Ex03;

public class Acao {
	
	
	public Acao() { //construtor vazio
		System.out.println("Construtor executado");
	}
	
	public Acao(String nome) {
		System.out.println("Bom dia, "+nome);
	}
	
	public Acao(String nome, int idade) {
		System.out.println("Fala "+nome+"! - "+idade+"? idoso(a)");
	}
	
}
