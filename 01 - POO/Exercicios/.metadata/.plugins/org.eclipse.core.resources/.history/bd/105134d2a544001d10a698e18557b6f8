package exPOO05;

public class Main {
	
	public static void main(String[] args) {
		char[] a = new char[10];
		String str = "";
		for (int i=0; i<10;i++) {
			str +="letra em "+i+"="+a[i]+"\n";
			if (a[i]==' ') {
				System.out.println("danonao");
			}
		}
		System.out.println(str);
		System.out.println(a.toString());
	}
		
}
