package exPOO07;

import javax.swing.JOptionPane;

public class Main {

	public static void main(String[] args) {
		BatalhaNaval bn = new BatalhaNaval();
		try {
		int posicao = Integer.parseInt(JOptionPane.showInputDialog(null, "Batalha naval\n\n"+bn.mostraMar()+"\n\nInforme a casa que você quer tentar (linha e coluna)"));
		} catch(NumberFormatException e){		
			JOptionPane.showMessageDialog(null, "Você deve informar um número de 2 digitos entre 00 e 99");
		}
		
		
	}
	
}
